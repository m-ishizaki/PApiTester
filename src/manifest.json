{
    "manifest_version": 2,
    "name": "PApiTester",
    "version": "0.0.0.1",

    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": [
            "http://*/*",
            "https://*/*"
          ],
          "css": ["contents_style.css"],
          "js": ["content_scripts_common.js"]
        }
    ],
    "permissions": [
        "tabs",
        "https://*/*",
        "http://*/*"
    ]
}